{
  "manifest_version": 2,
  "name": "CSGORoll Clicker",
  "version": "1.0",
  "description": "Clicks on the first item on CSGORoll withdraw.",
  "permissions": [
    "activeTab"
  ],
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": ["https://www.csgoroll.com/en/withdraw/csgo/p2p"],
      "js": ["content.js"]
    }
  ]
}
